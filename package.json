    "@vue/vue3-jest": "^29.2.2",
    "axios": "^1.3.2",
    "pinia": "^2.0.28",
    "sass": "^1.58.0",
    "swiper": "^9.0.3",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vuetify": "^3.1.3"
  },
  "devDependencies": {
    "@pinia/testing": "0.0.14",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jest": "^29.4.0",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
