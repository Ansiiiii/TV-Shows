    "eslint-plugin-vue": "^9.3.0",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.4.2",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.15.2",
    "ts-jest": "^29.0.5",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vitest": "^0.25.6",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^1.0.12"
